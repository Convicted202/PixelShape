{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/components/**",
    "src/containers/**",
    "src/polyfills/**",
    "src/helpers/**",
    "src/libs/**"
  ],
  "require": [
    "babel-register"
  ],
  "reporter": [
