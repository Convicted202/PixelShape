.canvas-anchors

  &-group
    list-style: none
    height: 32px
    padding: 0
    margin: 0
    border-bottom: 1px solid #e0e0e0

    &:last-child
      border-bottom: none

    .canvas-anchor
      display: inline-block
      width: 32px
      height: 32px
      padding: 3px
      cursor: pointer
      border-right: 1px solid #e0e0e0

      &:last-child
        border-right: none

      &:hover
        background-color: lightgray

      &__icon
        padding: 7px
        fill: #264653
