{
  "name": "pixelshape",
  "version": "0.0.1",
  "description": "Pixel editor that comes in handy when creating pixel art images and animations",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "prestart": "npm run clean",
    "start": "webpack-dev-server",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lint": "eslint ./src",
    "pretest": "npm run lint",
    "test": "cross-env NODE_PATH=src babel-node node_modules/blue-tape/bin/blue-tape test/**/*.spec.js",
    "test:dev": "npm test | faucet || true"
  },
  "keywords": [
    "pixel",
    "artwork",
    "editor",
    "online",
    "8bit"
  ],
  "author": "Alexander Yanovych",
  "license": "MIT",
  "devDependencies": {
    "@eleks/eslint-config-eleks": "^0.1.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "blue-tape": "^1.0.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "open": "0.0.5",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "worker-loader": "^0.7.1"
  },
  "dependencies": {
    "file-saver": "^1.3.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  }
}
